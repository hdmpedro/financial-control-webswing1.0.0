      "contentSecurityPolicy" : "default-src 'self' data:; script-src 'self' blob:; connect-src 'self' ws: wss: data: blob:; worker-src 'self' blob:; media-src 'self' blob: data:; frame-src 'self' https://www.youtube.com https://www.openstreetmap.org"
    },
    "adminConsoleUrl" : "${webswing.admin.url}",
    "webFolder": "${webswing.rootDir}/apps/selector"
