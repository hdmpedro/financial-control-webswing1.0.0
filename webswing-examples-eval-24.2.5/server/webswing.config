{
  "/" : {
    "path" : "/",
    "security" : {
      "module" : "NONE"
    },
    "langFolder" : "${webswing.configDir}/lang",
    "homeDir" : "${user.dir}",
    "webSecurity" : {
      "contentSecurityPolicy" : "default-src 'self' data:; script-src 'self' blob:; connect-src 'self' ws: wss: data: blob:; worker-src 'self' blob:; media-src 'self' blob: data:; frame-src 'self'"
    },
    "webFolder": "${webswing.rootDir}/apps/selector",
    // Adicionado contexto público para o seletor de aplicações
    "publicUrl": "${server.publicUrl}"
  },
  "/financial-control" : {
    "path" : "/financial-control",
    "security" : {
      "module" : "NONE"
    },
    "enabled" : true,
    "name" : "Financial Control Application",
    "webFolder" : "${webswing.rootDir}/apps/financial-control/webroot",
    "icon" : "icon.png",
    "maxClients" : 10,
    "sessionMode" : "CONTINUE_FOR_BROWSER",
    // Adicionado contexto público para a aplicação
    "publicUrl": "${server.publicUrl}/financial-control",
    "swingConfig" : {
      "allowUpload" : true,
      "allowDownload" : true,
      "isolatedFs" : false,
      "debug" : true,
      "homeDir" : "${webswing.rootDir}/apps/financial-control",
      "jreExecutable" : "${java.home}/bin/java",
      "vmArgs" : "-Xmx512m -Djava.awt.headless=false",
      "launcherType" : "Desktop",
      "launcherConfig" : {
        "mainClass" : "io.hdmpedro.financeiro.FinancialControlApplication",
        // Adicionar propriedade para URL pública
        "properties": {
          "webswing.publicUrl": "${server.publicUrl}"
        }
      },
      "classPathEntries" : [ 
        "${webswing.rootDir}/apps/financial-control/financial-control-webswing-1.0-SNAPSHOT.jar"
      ],
      "swingSessionTimeout" : 300,
      "allowDelete" : true,
      "allowLocalClipboard" : true
    }
  }
}