{
  "/" : {
    "path" : "/",
    "security" : {
      "module" : "org.webswing.security.modules.demo.DemoSecurityModule",
      "classPath" : [ "${webswing.rootDir}/security/*" ]
    },
    "langFolder" : "${webswing.configDir}/lang",
    "homeDir" : "${user.dir}",
    "webSecurity" : {
      "contentSecurityPolicy" : "default-src 'self' data:; script-src 'self' blob:; connect-src 'self' ws: wss: data: blob:; worker-src 'self' blob:; media-src 'self' blob: data:; frame-src 'self'"
    },
    "adminConsoleUrl" : "${webswing.admin.url}",
    "webFolder": "${webswing.rootDir}/apps/selector"
  },
  "/financial-control" : {
    "path" : "/financial-control",
    "enabled" : true,
    "name" : "Financial Control Application",
    "webFolder" : "${webswing.rootDir}/apps/financial-control/webroot",
    "restrictedResources" : [ ],
    "langFolder" : "",
    "security" : {
      "module" : "INHERITED",
      "config" : { },
      "authorizationConfig" : {
        "users" : [ ],
        "roles" : [ ]
      }
    },
    "webHomeDir" : "${webswing.rootDir}/apps/financial-control",
    "icon" : "icon.png",
    "uploadMaxSize" : 5,
    "maxClients" : 100,
    "sessionMode" : "CONTINUE_FOR_BROWSER",
    "monitorEdtEnabled" : true,
    "recordingConsentRequired" : false,
    "mirroringConsentRequired" : false,
    "loadingAnimationDelay" : 2,
    "allowStealSession" : true,
    "autoLogout" : false,
    "goodbyeUrl" : "/",
    "dataStore" : {
      "module" : "INHERITED",
      "config" : { }
    },
    "swingConfig" : {
      "allowUpload" : true,
      "allowDownload" : true,
      "isolatedFs" : true,
      "sessionLogging" : false,
      "allowJsLink" : true,
      "javaFx" : false,
      "homeDir" : "${webswing.rootDir}/apps/financial-control",
      "theme" : "Murrine",
      "directdraw" : true,
      "debug" : true,
      "jreExecutable" : "${java.home}/bin/java",
      "javaVersion" : "${java.version}",
      "vmArgs" : "-Xmx512m",
      "launcherType" : "Desktop",
      "launcherConfig" : {
        "mainClass" : "io.hdmpedro.financeiro.FinancialControlApplication"
      },
      "swingSessionTimeout" : 300,
      "timeoutIfInactive" : false,
      "allowDelete" : true,
      "allowAutoDownload" : true,
      "allowLocalClipboard" : true,
      "allowServerPrinting" : false,
      "dockMode" : "ALL",
      "allowStatisticsLogging" : true,
      "testMode" : false,
      "jsLinkWhitelist" : [ "*" ],
      "transferDir" : "${user}/upload",
      "clearTransferDir" : true,
      "sessionLogFileSize" : "${webswing.sessionLog.size:-10MB}",
      "transparentFileSave" : true,
      "sessionLogMaxFileSize" : "${webswing.sessionLog.maxSize:-1000MB}",
      "transparentFileOpen" : true,
      "fontConfig" : { },
      "classPathEntries" : [ "financial-control-webswing-1.0-SNAPSHOT.jar" ],
      "multiScreenSupportEnabled" : true
    }
  }
}